{
  "redirects": [
    {
      "source": "/",
      "destination": "/en/",
      "permanent": false
    },
    {
      "source": "/:path((?![a-z]{2}).*)",
      "destination": "/en/:path",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/_astro/:all*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
